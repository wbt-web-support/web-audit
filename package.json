{
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --prefix \"[{name}]\" --names \"Next.js,Workers\" \"next dev\" \"npm run start:workers:dev\"",
    "dev:app": "next dev",
    "dev:workers": "npm run start:workers:dev",
    "dev:custom": "node scripts/dev-with-workers.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:workers": "tsx scripts/start-queue-workers.ts",
    "start:workers:dev": "tsx --watch scripts/start-queue-workers.ts",
    "optimize:100-users": "npx ts-node --esm scripts/optimize-for-100-users.ts",
    "optimize:200-users-8gb": "npx ts-node --esm scripts/optimize-for-200-users-8gb.ts",
    "optimize:current-server": "npx ts-node --esm scripts/optimize-current-server.ts",
    "optimize:100-users-fast": "npx ts-node --esm scripts/optimize-for-100-users-fast.ts",
    "optimize:100-users-simultaneous": "npx ts-node --esm scripts/optimize-simultaneous-100-users.ts",
    "optimize:100-users-staged": "npx ts-node --esm scripts/optimize-staged-100-users.ts",
    "optimize:32gb-memory": "npx ts-node --esm scripts/optimize-for-32gb-memory.ts",
    "simulate:100-workers": "npx ts-node --esm scripts/simulate-100-workers.ts",
    "update:scaling": "npx ts-node --esm scripts/update-scaling-config.ts",
    "validate:scaling": "npx ts-node -e \"import { dynamicScalingManager } from './lib/saas/config/dynamic-scaling'; const validation = dynamicScalingManager.validateScalingConfig(); console.log(validation.valid ? '✅ Configuration valid' : '❌ Configuration invalid:', validation.errors);\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.10.0",
    "bullmq": "^5.58.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.511.0",
    "next": "latest",
    "next-themes": "^0.4.6",
    "node-html-parser": "^7.0.1",
    "node-html-to-image": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "robots-parser": "^3.0.1",
    "tailwind-merge": "^3.3.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
